@prefix etudiant_thyp: <https://jardindesconnaissances.univ-paris8.fr/onto/etudiant_thyp_cabinet_Management#> .

<https://jardindesconnaissances.univ-paris8.fr/onto/etudiant_thyp> a owl:Ontology ;
    dcterms:title "Gestion de Cabinet Médical - Université Paris 8" .

### Classe Patient
etudiant_thyp:Patient a rdfs:Class ;
    rdfs:label "Patient" ;
    rdfs:comment "Représente un patient du cabinet médical" ;
    vs:term_status "experimental" .

etudiant_thyp:timeSubmit a rdf:Property ;
    rdfs:label "Date de soumission" ;
    rdfs:domain etudiant_thyp:Patient ;
    vs:term_status "experimental" .

etudiant_thyp:sexe a rdf:Property ;
    rdfs:label "Sexe" ;
    rdfs:domain etudiant_thyp:Patient ;
    vs:term_status "experimental" .

etudiant_thyp:age a rdf:Property ;
    rdfs:label "Age" ;
    rdfs:domain etudiant_thyp:Patient ;
    vs:term_status "experimental" .

etudiant_thyp:fume a rdf:Property ;
    rdfs:label "Fume" ;
    rdfs:domain etudiant_thyp:Patient ;
    vs:term_status "experimental" .

etudiant_thyp:sport a rdf:Property ;
    rdfs:label "Sport" ;
    rdfs:domain etudiant_thyp:Patient ;
    vs:term_status "experimental" .

etudiant_thyp:alcool a rdf:Property ;
    rdfs:label "Consommation d'Alcool" ;
    rdfs:domain etudiant_thyp:Patient ;
    vs:term_status "experimental" .

etudiant_thyp:mail a rdf:Property ;
    rdfs:label "Email" ;
    rdfs:domain etudiant_thyp:Patient ;
    vs:term_status "experimental" .

### Classe Cabinet
etudiant_thyp:Cabinet a rdfs:Class ;
    rdfs:label "Cabinet Médical" ;
    rdfs:comment "Représente un cabinet médical" ;
    vs:term_status "experimental" .

etudiant_thyp:nom a rdf:Property ;
    rdfs:label "Nom du Cabinet" ;
    rdfs:domain etudiant_thyp:Cabinet ;
    vs:term_status "experimental" .

etudiant_thyp:adresse a rdf:Property ;
    rdfs:label "Adresse" ;
    rdfs:domain etudiant_thyp:Cabinet ;
    vs:term_status "experimental" .

etudiant_thyp:specialite a rdf:Property ;
    rdfs:label "Spécialité Médicale" ;
    rdfs:domain etudiant_thyp:Cabinet ;
    vs:term_status "experimental" .

### Classe Appointment
etudiant_thyp:Appointment a rdfs:Class ;
    rdfs:label "Rendez-vous" ;
    rdfs:comment "Représente un rendez-vous médical" ;
    vs:term_status "experimental" .

etudiant_thyp:appointmentDate a rdf:Property ;
    rdfs:label "Date du Rendez-vous" ;
    rdfs:domain etudiant_thyp:Appointment ;
    vs:term_status "experimental" .

etudiant_thyp:phone a rdf:Property ;
    rdfs:label "Téléphone du Patient" ;
    rdfs:domain etudiant_thyp:Appointment ;
    vs:term_status "experimental" .

etudiant_thyp:isValidated a rdf:Property ;
